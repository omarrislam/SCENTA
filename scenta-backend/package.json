{
  "name": "scenta-backend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p tsconfig.build.json",
    "build:vercel": "tsc -p tsconfig.vercel.json && node -e \"require('fs').mkdirSync('api/dist-cjs',{recursive:true});require('fs').writeFileSync('api/dist-cjs/package.json', JSON.stringify({type:'commonjs'}));\"",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "start": "node dist/server.js",
    "seed": "tsx src/scripts/seed.ts",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test": "vitest run"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "express-rate-limit": "^7.4.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mongoose": "^8.9.1",
    "morgan": "^1.10.0",
    "nanoid": "^5.0.7",
    "@sendgrid/mail": "^8.1.3",
    "stripe": "^17.4.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.11",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.17.9",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "globals": "^15.11.0",
    "mongodb-memory-server": "^10.1.1",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.9"
  }
}
